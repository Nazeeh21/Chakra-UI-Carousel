{
  "name": "chakra-ui-carousel",
  "version": "0.2.0",
  "description": "An experiment for tight-knit Chakra+Formik integration with a familiar API",
  "keywords": [
    "chakra",
    "chakra-ui",
    "carousel"
  ],
  "main": "dist/chakra-ui-carousel.cjs.js",
  "module": "dist/chakra-ui-carousel.esm.js",
  "author": "Nazeeh Vahora",
  "homepage": "https://github.com/Nazeeh21/Chakra-UI-Carousel",
  "repository": "github:nazeeh21/chakra-ui-carousel",
  "bugs": {
    "url": "https://github.com/Nazeeh21/Chakra-UI-Carousel/issues"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "preconstruct build",
    "build:clean": "rimraf dist",
    "lint": "eslint ./src --ext .ts,.tsx",
    "release": "changeset publish",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "storybook": "start-storybook -p 9001 -s ./src/_/ .storybook",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic  --exit-zero-on-changes"
  },
  "peerDependencies": {
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.0.0",
    "formik": "^2.0.0",
    "framer-motion": "^6.2.8",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.16.7",
    "@chakra-ui/core": "^0.8.0",
    "@chakra-ui/icons": "^1.1.7",
    "@chakra-ui/react": "^1.6.3",
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@preconstruct/cli": "^2.1.0",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/react-hooks": "^5.0.3",
    "@testing-library/user-event": "^12.7.1",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "chromatic": "^6.5.1",
    "eslint": "^7.20.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.7",
    "formik": "^2.2.8",
    "framer-motion": "^4.1.17",
    "jest": "^26.6.3",
    "lint-staged": ">=10",
    "msw": "^0.35.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.5",
    "yup": "^0.32.9"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint --cache --fix",
      "prettier --write"
    ],
    "*.{yml,json,md}": "prettier --write"
  },
  "dependencies": {
    "rooks": "^5.10.3"
  }
